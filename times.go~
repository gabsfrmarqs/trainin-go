package main

import (
	"fmt"
	"time"
)

func main(){
	n := time.Now()
	fmt.Println(n)

	t := time.Date(2024, time.February, 27, 17, 19, 0, 0, time.UTC)
	fmt.Println("Look!: ", t)
	fmt.Println(t.Format(time.ANSIC))

	parsedTime := time.Parse(time.ANSIC, "Tue Nov 10 23:00:00 2009")
	fmt.Println(parsedTime)
}

